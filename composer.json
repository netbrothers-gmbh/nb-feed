{
  "name": "netbrothers-gmbh/nb-feed",
  "description": "Fetch RSS- and Atom-Feeds and save to JSON",
  "keywords": ["rss", "atom"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Stefan Wessel",
      "email": "info@netbrothers.de"
    },
    {
      "name": "Thilo Ratnaweera",
      "email": "info@netbrothers.de"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-simplexml": "*"
  },
  "autoload": {
    "psr-4": {
      "NetBrothers\\NbFeed\\": "src/"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2",
    "friendsofphp/php-cs-fixer": "^3"
  }
}
